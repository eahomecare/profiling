{
  "name": "customer-profile",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "add-random-customers": "node db-scripts/add_random_customers.js",
    "view-customer-crm-id-mappings": "node db-scripts/view-customer-crm-ids.js",
    "customer-crm-id-mappings-generate": "node db-scripts/customer-crm-ids-generate.js",
    "delete-customer-crm-id-mappings": "node db-scripts/delete-customer-crm-ids.js",
    "view-keywords": "node db-scripts/view-keywords.js",
    "transfer-keywords": "node db-scripts/transfer-keywords.js",
    "delete-keywords": "node db-scripts/delete-keywords.js",
    "prisma:dev:deploy": "npx prisma db push",
    "prisma:test:deploy": "dotenv -e .env.test -- prisma db push",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "pretest:e2e": "npm run prisma:test:deploy",
    "test:e2e": "dotenv -e .env.test -- jest --watch --no-cache --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.17.12",
