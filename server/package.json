    "lodash": "^4.17.21",
    "luxon": "^3.3.0",
    "mongodb": "^5.4.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.9.4",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "prisma": "^4.11.0",
    "readline": "^1.3.0",
